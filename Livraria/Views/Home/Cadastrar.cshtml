@model Cliente
<form method="post">

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <H2> <label style="color:darkorange; margin-left:10px"><b><u>Dados Pessoais:</u></b></label></H2>
    <br />
    <br />

    <H4> <label style="color:white; margin-left:10px"><b><u>Cadastro do CLIENTE:</u></b></label></H4>
    <div class="form-group" id="Cliente">
        <div class="form-group">
            <div class="form-inline">
                <input class="form-control col-md-4" style="margin-left:10px;" placeholder="Nome" asp-for="Nome" title="Nome" required="" />
            </div>
            <br />

            <div class="form-group">
                <div class="form-inline">
                    <input class="form-control col-md-4" style="margin-left:10px;" placeholder="Sobrenome" asp-for="Sobrenome" title="Sobrenome" required="" />
                </div>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="form-inline">
            <input class="form-control col-md-2" style="margin-left:10px;" placeholder="CPF" asp-for="Cpf" title="CPF" required="" />
            <input type="password" class="form-control col-md-2" style="margin-left:10px;" placeholder="Senha" asp-for="Senha" title="Senha" required="" />
            <input  class="form-control col-md-2" style="margin-left:10px;" placeholder="Rg" asp-for="Rg" title="Rg" required="" />
        </div>
    </div>
    <div class="form-group">
        <div class="form-inline">
            <input type="date" class="form-control col-md-2" style="margin-left:10px;" placeholder="Date" asp-for="DataNascimento" title="Date" required="" />
            <input type="tel" class="form-control col-md-2" style="margin-left:10px;" placeholder="Telefone" asp-for="Telefone" title="Telefone" required="" />

        </div>
    </div>
    <div class="form-group">
        <div class="form-inline">
            <input type="email" class="form-control col-md-4" style="margin-left:10px;" placeholder="Email" asp-for="Email" title="Email" required="" />
        </div>
    </div>


    <H4> <label style="color:white; margin-left:10px"><b><u>Cadastro do CEP:</u></b></label></H4>
    <div class="form-group" id="CEP">
        <div class="form-group">
            <div class="form-inline">

                <input class="form-control col-md-2" style="margin-left:10px;" placeholder="CEP" asp-for="Endereco.Cep" title="CEP"  />


                <input type="submit" value="Buscar" class="btn-outline-info form-control col-md-1" style="margin-left:10px;" asp-action="BuscarCep" asp-controller="Home" />
            </div>
        </div>
        <div class="form-group">
            <div class="form-inline">
                <div>
                    <label class="text-info"></label>
                </div>
                <input class="form-control col-md-2" style="margin-left:10px;" placeholder="Rua" asp-for="Endereco.endereco" title="Rua" readonly="" />
                <div>
                    <label class="text-info"></label>
                </div>
                <input class="form-control col-md-2" style="margin-left:10px;" placeholder="Bairro" asp-for="Endereco.Bairro" readonly="" title="Bairro" />
            </div>
        </div>
        <div class="form-group">
            <div class="form-inline">
                <div>
                    <label class="text-info"></label>
                </div>
                <input class="form-control col-md-2" style="margin-left:10px;" placeholder="Cidade" asp-for="Endereco.Cidade" title="Cidade" readonly="" />
                <div>
                    <label class="text-info"></label>
                </div>
                <input class="form-control col-md-2" style="margin-left:10px;" placeholder="Estado" asp-for="Endereco.Uf" readonly="" title="Estado" />
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="form-inline">
            <input type="submit" value="Cadastar" class="btn-outline-success form-control col-md-4" style="margin-left:10px;" asp-action="Cadastrar" asp-controller="Home" />
        </div>
    </div>


    <style>
        body {
            position: fixed;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            background: url(https://i.imgur.com/u9sUbQi.jpg) no-repeat center center fixed;
            background-size: cover;
            background-position: center;
            -webkit-filter: blur(px);
        }
    </style>
</form>






