@model List<Cliente>

<table class="table table-hover table-striped">
    <thead>
        <tr class="table-dark">
            <th>Nome</th>
            <th>Cpf</th>
            <th>Senha</th>
            <th>Data de Nascimento</th>
            <th>Email</th>
            <th>Telefone</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Cliente item in Model)
        {
        <tr class="table-light">
            <td>@item.Nome</td>
            <td>@item.Cpf</td>
            <td>@item.Senha</td>
            <td>@item.DataNascimento</td>
            <td>@item.Email</td>
            <td>@item.Telefone</td>
        


            <td>
                <a class="btn btn-outline-danger" asp-action="RemoverCliente" asp-route-id="@item.ClienteId">Remover</a>
            </td>
            <td>
                <a class="btn btn-outline-info" asp-action="EditarCliente" asp-route-id="@item.ClienteId">Alterar</a>

            </td>
        </tr>
        }
    </tbody>
</table>